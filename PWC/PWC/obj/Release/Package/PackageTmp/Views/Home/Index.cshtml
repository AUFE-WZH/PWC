@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>AUFE OnlineJudge - AUFEOJ - 安徽财经大学OJ系统 - AUFEACM</title>
    
    <link href="~/Content/bootstrap-3.3.7-dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/sp/sp.css" rel="stylesheet" />
    <script src="~/Scripts/Chart.js"></script>

    <style type="text/css">
        #kaiguan {
            position: fixed;
            top: 290px;
            left: 161px;
            z-index: 1001;
            text-align: center;
            border: 1px solid pink;
            border-radius: 5px;
            border-left: 0px;
            padding: 15px;
        }
        #kaiguan2 {
            position: fixed;
            top: 290px;
            right: 161px;
            z-index: 1001;
            text-align: center;
            border: 1px solid #bce8f1;
            border-radius: 5px;
            border-right: 0px;
            padding: 15px;
        }
    </style>
</head>
<body>

    <!--主要内容开始-->
    <div class="container " >
        
        <div class="row ">

            <!-- 导航条开始 -->
            <nav class="navbar navbar-default navbar-fixed-top container" >
                <div class="navbar-header">
                    <a href="Index" class="navbar-brand"><strong>AUFEOJ</strong></a>
                    <button class="navbar-toggle" data-toggle="collapse" data-target="#nav1">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>

                <div id="nav1" class="collapse navbar-collapse  ">

                    <ul class="nav navbar-nav ">
                        <li class="active"><a href="#"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>训练</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-fire" aria-hidden="true"></span>竞赛</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-list" aria-hidden="true"></span>排名</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-check" aria-hidden="true"></span>状态</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-question-sign" aria-hidden="true"></span>讨论</a></li>
                        <li><a href="#"><span class="glyphicon glyphicon-wrench" aria-hidden="true"></span>关于</a></li>
                    </ul>

                    <form action="" class="navbar-form navbar-right">
                        <input type="text" placeholder="搜索题目ID, 标题, 描述" class="form-control" />
                        <button type="submit" class="btn btn-default">提交</button>
                    </form>
                    <button type="button" class="navbar-btn btn btn-sm btn-primary navbar-right" style="margin-left: 10px; " data-toggle="modal" data-target="#myModal" id="login">登陆</button>
                    <button type="button" class="navbar-btn btn btn-sm btn-primary navbar-right" id="register">注册</button>
                </div>
            </nav>
            <!-- 导航条结束 -->
            <!--登录模态框开始 -->
            <div>
                <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                <h4 class="modal-title" id="myModalLabel">登录</h4>
                            </div>
                            <form>
                                <div class="modal-body">

                                    <div class="form-horizontal">
                                        <div class="form-group">
                                            <label for="UserName" class="col-sm-2 control-label">用户名</label>
                                            <div class="col-sm-10">
                                                <input type="text" class="form-control" id="UserName" placeholder="UserName">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="Password" class="col-sm-2 control-label">密码</label>
                                            <div class="col-sm-10">
                                                <input type="password" class="form-control" id="Password" placeholder="Password">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-offset-2 col-sm-10">
                                                <div class="checkbox">
                                                    <label>
                                                        <input type="checkbox" id="remember">记住
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-default" id="forget">忘记</button>
                                    <button type="submit" class="btn btn-primary" id="signin">登录</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!--登录模态框结束  -->
            <!-- 导航条结束 -->
        </div>
        <!--轮播开始-->
        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel" style="margin-top:70px">
            <ol class="carousel-indicators">
                <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                <li data-target="#carousel-example-generic" data-slide-to="3"></li>
            </ol>

            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="~/Content/img/1.jpg" alt="风景1" />
                    <div class="carousel-caption">
                        <h4>真正的才智是刚毅的志向。 —— 拿破仑</h4>
                    </div>
                </div>
                <div class="item">
                    <img src="~/Content/img/2.jpg" alt="风景2" />
                    <div class="carousel-caption">
                        <h4>聪明在于勤奋，天才在于积累。 —— 华罗庚</h4>
                    </div>
                </div>
                <div class="item">
                    <img src="~/Content/img/3.jpg" alt="风景3" />
                    <div class="carousel-caption">
                        <h4>志向和热爱是伟大行为的双翼。 —— 歌德</h4>
                    </div>
                </div>
                <div class="item">
                    <img src="~/Content/img/4.jpg" alt="风景4" />
                    <div class="carousel-caption">
                        <h4>天才是百分之一的灵感加百分之九十九的汗水。 —— 爱迪生</h4>
                    </div>
                </div>
            </div>

            <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
        <!--轮播结束-->
        <!--图表显示每日人数开始-->
        <div>
            <canvas id="chart-line" class="chartjs" width="1000" height="300"></canvas>
        </div>
        <!--图表显示每日人数结束-->

        <!--内容面板开始-->
        <div class="row" style="margin-top: 10px;" >
            <div>   
                <div class="col-md-4">
                    <!--公告面板开始-->
                    <div class="panel panel-primary" >
                        <div class="panel-heading " style="text-shadow: black 5px 3px 3px;">
                            <h3 class="panel-title"><span class="glyphicon glyphicon-edit" aria-hidden="true"></span> 公告</h3>
                        </div>
                        <div class="panel-body">
                            <strong>每天AC一道题，一年365道题，你就能成为一个编程高手！！！</strong>
                        </div>
                        <ul class="list-group">
                            <li class="list-group-item">新手注意事项：xxxxxx</li>
                            <li class="list-group-item">近期比赛时间：xxxxxxx</li>
                            <li class="list-group-item">比赛群号：xxxxxxxx</li>
                            <li class="list-group-item">安财ACM微博：xxxxxx</li>
                            <li class="list-group-item">公告5</li>
                        </ul>
                    </div>
                    <!--公告面板结束-->
                    <!--程序运行情况占比开始-->
                    <div>
                        <canvas id="chart-doughnut" class="chartjs" width="300" height="220"></canvas>
                    </div>
                    <!--程序运行情况占比结束-->
                </div> 
                <!--刷题排行榜开始-->
                <div class="col-md-8">
                    <div class="panel panel-primary">
                        <div class="panel-heading" style="text-shadow: black 5px 3px 3px;">
                            <h3 class="panel-title"><span class="glyphicon glyphicon-star" aria-hidden="true"></span> 最佳ACMers</h3>
                        </div>
                        <div class="panel-body">
                            <strong> 本周刷题排行榜！！！</strong>
                        </div>
                        <ul class="list-group">
                            <li class="list-group-item">公告1</li>
                            <li class="list-group-item">公告2</li>
                            <li class="list-group-item">公告3</li>
                            <li class="list-group-item">公告4</li>
                            <li class="list-group-item">公告5</li>
                        </ul>
                    </div>
                </div>
                <!--刷题排行榜结束-->             
            </div>
            <div>
                <!--好题排行榜开始-->
                <div class="col-md-8">
                    <div class="panel panel-primary">
                        <div class="panel-heading" style="text-shadow: black 5px 3px 3px;">
                            <h3 class="panel-title"><span class="glyphicon glyphicon-flag" aria-hidden="true"></span> 最佳Problems</h3>
                        </div>
                        <div class="panel-body">
                            <strong> 本周好题排行榜！！！</strong>
                        </div>
                        <ul class="list-group">
                            <li class="list-group-item">公告1</li>
                            <li class="list-group-item">公告2</li>
                            <li class="list-group-item">公告3</li>
                            <li class="list-group-item">公告4</li>
                            <li class="list-group-item">公告5</li>
                        </ul>
                    </div>
                </div>
                <!--好题排行榜结束-->
            </div>
        </div>
        <!--内容面板结束-->
        
    </div>
    <!--主要内容结束-->

    <!--左侧浮动图片开始-->
    <div id="zuoce" style="position: fixed; top: 150px; left: 1px; z-index: 1001; text-align: center;">
        <div class="panel panel-danger hidden-md hidden-sm hidden-xs">
            <div class="panel-heading">
                <h3 class="panel-title"><span class="glyphicon glyphicon-link" aria-hidden="true"></span><strong>其他训练平台</strong></h3>
            </div>
            <div class="panel-body">
                <a href="http://acm.nyist.edu.cn/JudgeOnline/problemset.php">南阳理工学院OJ</a>
            </div>
            <div class="panel-body">
                <a href="http://poj.org/problemlist">北京大学OJ</a>
            </div>
            <div class="panel-body">
                <a href="http://openjudge.cn/">百炼OJ</a>
            </div>
            <div class="panel-body">
                <a href="http://acm.hdu.edu.cn/">杭州电子科技大学OJ</a>
            </div>
            <div class="panel-body">
                <a href="http://bestcoder.hdu.edu.cn/">bestcoderOJ</a>
            </div>
            <div class="panel-body">
                <a href="http://acm.hpu.edu.cn/">河南理工大学OJ</a>
            </div>
        </div>
    </div>
    <div id="kaiguan" class="hidden-md hidden-sm hidden-xs"  onclick="kgdisplay()">
        <span id="kg" class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    </div>
    <!--左侧浮动图片结束-->

    <!--右上侧浮动图片开始-->
    <div id="youce" style="position: fixed; top: 150px; right: 0px; z-index: 1001; text-align: center;">
        <div class="panel panel-info hidden-md hidden-sm hidden-xs">
            <div class="panel-heading">
                <h3 class="panel-title"><span class="glyphicon glyphicon-phone" aria-hidden="true"></span><strong>轻松时刻</strong></h3>
            </div>
            <div class="panel-body">
                <a href="/Home/Gobang">五子棋</a>
            </div>
            <div class="panel-body">
                <a href="/Home/Pinball">弹球游戏</a>
            </div>
            <div class="panel-body">
                <a href="/Home/Snake">贪吃蛇</a>
            </div>
            <div class="panel-body">
                <a href="https://www.xctf.org.cn/">CTF比赛</a>
            </div>
            <div class="panel-body">
                <a href="http://www.ruanyifeng.com/home.html">编程大牛阮一峰</a>
            </div>
            <div class="panel-body">
                <a href="https://www.liaoxuefeng.com/">学习园地廖雪峰</a>
            </div>
        </div>
    </div>
    <div id="kaiguan2" class="hidden-md hidden-sm hidden-xs" onclick="kgdisplay2()">
        <span id="kg2" class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    </div>
    <!--右上侧浮动图片结束-->
    
    <!--右下侧浮动图片开始-->
    <div id="sp" class="hidden-md hidden-sm hidden-xs">
        <div id="sp-wrapper" style="display:block;">
            <div id="sp-msgbox">
                <div id="sp-msg"></div>
                <div id="sp-msgborder"></div>
            </div>
            <div id="sp-body">
                <img id="sp-img" src="~/Content/sp/online.png" />
            </div>
        </div>
        <div id="sp-bottom">
            <span id="sp-shown-hidden">关闭</span>
        </div>
    </div>
    <!--右下侧浮动图片结束-->

    <!--页脚内容开始-->
    <div class="footer">
        <table class="table-responsive" align="center">
            <tbody>
                <tr>
                    <td  style="padding:40px ; padding-left:5px">
                        <img src="~/Content/img/AUFE-WeChat.png" />
                    </td>
                    <td align="center" style="padding-left: 40px;">
                        Anything about AUFE OnlineJudge, Please Contact Admin:
                        <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=464407764&site=qq&menu=yes">王智豪</a>
                        <br />
                        © All Copyright Reserved 2010-2025
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <!--页脚内容结束-->

    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Content/bootstrap-3.3.7-dist/js/bootstrap.js"></script>
    <script src="~/Content/sp/sp.js"></script>

    <script>  

        var chartline = document.getElementById("chart-line").getContext('2d');
        var ChartLine = new Chart(chartline, {
            // 要创建的图表类型
            type: 'line',

            // 数据集
            data: {
                labels: ["21", "22", "23", "24", "25", "26", "27"],
                datasets: [{
                    label: "答题量",
                    data: [0, 10, 5, 2, 20, 30, 45],
                    backgroundColor: 'rgba(20 ,229, 238, 0.2)',
                    borderColor: 'rgb(0, 0, 0)',
                    borderWidth: 1,
                
                    
                }, {
                    label: '登录人数',
                    data: [12, 19, 3, 5, 2, 3,46],
                    backgroundColor: 'rgba(255, 99, 132, 0.2)',
                    borderColor:'rgba(255,99,132,1)',
                    borderWidth: 1

                }],

            },

            // 配置项
            options: {
            }

        });
    </script>

    <script>
        var chartdoughnut = document.getElementById("chart-doughnut").getContext('2d');
        var ChartDoughnut = new Chart(chartdoughnut, {
            // 要创建的图表类型
            type: 'doughnut',

            // 数据集
            data: {
                labels: [
                    'Accepted',
                    'WrongAnswer',
                    'RunTimeError',
                    'TimeLimitExceeded',
                    'PresentationError',
                    'MemoryLimitError',
                    'CompileError'
                ],
                datasets: [{
                    label: "程序运行情况占比",
                    data: [10, 10, 10, 10, 10, 10, 10],
                    backgroundColor: [
                        'rgb(0, 0, 255 )',
                        'rgb(255, 0, 0)',
                        'rgb(255, 255, 0)',
                        'rgb(0, 255, 0)',
                        'rgb(0, 127, 255)',
                        'rgb(255, 165, 0)',
                        'rgb(139, 0, 255)',
                    ]
                }]
            },

            // 配置项
            options: {
            }

        });

    </script>

    <script>
        var zuoce = $("#zuoce");
        var kaiguan = $("#kaiguan");
        var kg = $("#kg");
        var kai = "glyphicon glyphicon-chevron-right";
        var guan = "glyphicon glyphicon-chevron-left";
        function kgdisplay() {
            if (kg.attr('class') == guan) {
                kg.attr('class', kai);
                zuoce.hide();
                kaiguan.css({ position: "fixed", 'top': 290, 'left': -1, 'z-index': 1001, "border": "1px solid pink" });

            }
            else {
                kg.attr('class', guan);
                zuoce.show();
                kaiguan.css({ position: "fixed", 'top': 290, 'left': 161, 'z-index': 1001, "border": "1px solid pink", "border-left": "0" });
            }
        }
    </script>

    <script>
        var youce = $("#youce");
        var kaiguan2 = $("#kaiguan2");
        var kg2 = $("#kg2");
        var kai = "glyphicon glyphicon-chevron-right";
        var guan = "glyphicon glyphicon-chevron-left";
        function kgdisplay2() {
            if (kg2.attr('class') == guan) {
                kg2.attr('class', kai);
                youce.hide();
                kaiguan2.css({ position: "fixed", 'top': 290, 'right': -1, 'z-index': 1001, "border": "1px solid #bce8f1" });

            }
            else {
                kg2.attr('class', guan);
                youce.show();
                kaiguan2.css({ position: "fixed", 'top': 290, 'right': 161, 'z-index': 1001, "border": "1px solid #bce8f1", "border-right": "0" });
            }
        }
    </script>
</body>
</html>
